<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="ThemeBackgroundColor">#0F0F0F</Color>
    <Color x:Key="ThemeBorderLowColor">#1E1E1E</Color>
    <Color x:Key="ThemeBorderMidColor">#232323</Color>
    <Color x:Key="ThemeBorderHighColor">#282828</Color>
    <Color x:Key="ThemeControlLowColor">#0A0A0A</Color>
    <Color x:Key="ThemeControlMidColor">#0F0F0F</Color>
    <Color x:Key="ThemeControlMidHighColor">#141414</Color>
    <Color x:Key="ThemeControlHighColor">#191919</Color>
    <Color x:Key="ThemeControlVeryHighColor">#1E1E1E</Color>
    <Color x:Key="ThemeControlHighlightLowColor">#232323</Color>
    <Color x:Key="ThemeControlHighlightMidColor">#282828</Color>
    <Color x:Key="ThemeControlHighlightHighColor">#2D2D2D</Color>
    <Color x:Key="ThemeForegroundLowColor">#A0A0A0</Color>
    <Color x:Key="ThemeForegroundMidColor">#BEBEBE</Color>
    <Color x:Key="ThemeForegroundHighColor">#DCDCDC</Color>

    <!-- Output type colors -->
    <Color x:Key="InformationOutputColor">#4F84E2</Color>
    <Color x:Key="WarningOutputColor">#F2B335</Color>
    <Color x:Key="ErrorOutputColor">#F03649</Color>
</ResourceDictionary>